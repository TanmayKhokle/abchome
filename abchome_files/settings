        sTag.type = "text/javascript";
        sTag.async = nv.async;
        Object.keys(eAtt).forEach(function (el){
            sTag[el] = eAtt[el];
        });
        sTag.src = link;
        if (d.body) {
            d.body.appendChild(sTag);
        } else {
            d.head.appendChild(sTag);
        }
    };


 
